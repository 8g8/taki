{
  "name": "taki",
  "version": "1.3.1",
  "description": "Take a snapshot of any website.",
  "repository": {
    "url": "egoist/taki",
    "type": "git"
  },
  "bin": "dist/cli.js",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "keywords": [
    "snapshot",
    "prerender",
    "crawler"
  ],
  "scripts": {
    "test": "ava",
    "prepublishOnly": "npm run build",
    "build": "rm -rf dist && tsup src/{index,cli}.ts --dts"
  },
  "author": "egoist <0x142857@gmail.com>",
  "license": "MIT",
  "jest": {
    "testEnvironment": "node"
  },
  "dependencies": {
    "cac": "6.6.1",
    "debug": "4.1.1",
    "html-minifier": "4.0.0",
    "puppeteer": "5.2.1"
  },
  "devDependencies": {
    "@egoist/prettier-config": "^0.1.0",
    "@types/debug": "^4.1.5",
    "@types/html-minifier": "^4.0.0",
    "@types/node": "^14.0.27",
    "@types/polka": "^0.5.1",
    "@types/puppeteer": "^3.0.1",
    "ava": "3.11.0",
    "polka": "^0.5.2",
    "sucrase": "^3.15.0",
    "tsup": "^3.4.2",
    "typescript": "^3.9.7"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "sucrase/register"
    ]
  }
}
