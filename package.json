{
  "name": "taki",
  "version": "1.2.3",
  "description": "Take a snapshot of any website.",
  "repository": {
    "url": "egoist/taki",
    "type": "git"
  },
  "bin": "dist/cli.js",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "keywords": [
    "snapshot",
    "prerender",
    "crawler"
  ],
  "scripts": {
    "test": "npm run lint && ava",
    "lint": "xo",
    "prepublishOnly": "npm run build",
    "build": "bili src/{index,cli}.js --filename [name].js --target node --no-babel.babelrc"
  },
  "author": "egoist <0x142857@gmail.com>",
  "license": "MIT",
  "jest": {
    "testEnvironment": "node"
  },
  "dependencies": {
    "cac": "^4.4.3",
    "debug": "^3.1.0",
    "html-minifier": "^3.5.8",
    "puppeteer": "^1.1.1"
  },
  "devDependencies": {
    "ava": "^0.21.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "bili": "^3.0.4",
    "eslint-config-rem": "^3.0.0",
    "xo": "^0.18.0"
  },
  "xo": {
    "extends": "rem"
  },
  "ava": {
    "babel": "inherit",
    "require": [
      "babel-register"
    ]
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ]
  }
}
