{
  "name": "taki",
  "version": "1.0.0",
  "description": "Take a snapshot of any website.",
  "repository": {
    "url": "egoist/taki",
    "type": "git"
  },
  "main": "dist/taki.js",
  "files": [
    "dist"
  ],
  "keywords": [
    "snapshot",
    "prerender",
    "crawler"
  ],
  "scripts": {
    "test": "npm run lint && ava",
    "lint": "xo",
    "prepublishOnly": "npm run build",
    "build": "bili --filename taki.js --buble.target.node 6"
  },
  "author": "egoist <0x142857@gmail.com>",
  "license": "MIT",
  "jest": {
    "testEnvironment": "node"
  },
  "dependencies": {
    "puppeteer": "^0.13.0"
  },
  "devDependencies": {
    "ava": "^0.21.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "bili": "^1.4.2",
    "eslint-config-rem": "^3.0.0",
    "xo": "^0.18.0"
  },
  "xo": {
    "extends": "rem"
  },
  "ava": {
    "babel": "inherit",
    "require": [
      "babel-register"
    ]
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ]
  }
}
